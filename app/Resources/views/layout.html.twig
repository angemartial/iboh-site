    <div class="wrapper white_bg">
        <!--Header section start-->
        <header class="header">
            <div class="header-top-1">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-4 hidden-xs">

                        </div>
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <div class="header-1-top-inner">
                                <div class="header-topbar-menu">
                                    <ul>
                                        <li><span><img src="img/icon/mail.png" alt=""></span></li>
                                        <li><a href="agency.html"></a></li>
                                        <li><a data-target="#quick-view" data-toggle="modal" href="{{ path('login') }}">Se connecter</a></li>
                                        <li><a href="{{ path('register') }}">S'inscrire</a></li>
                                    </ul>
                                </div>
                                <div class="header-search">
                                    <div class="search-icon">
                                        <a href="#"><img src="img/icon/search.png" alt=""></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-top sticky-header">
                <div class="container">
                    <div class="row">
                        <div class="col-md-2 col-sm-4 col-xs-6">
                            <div class="logo">
                                <a href="{{ path('index') }}"><img src="{{ asset('img/logo/logo1.fw.png') }}" alt=""></a>
                            </div>
                        </div>
                        <div class="col-md-10 hidden-sm hidden-xs">
                            <div class="mgea-full-width">
                                <div class="header-menu">
                                    <nav>
                                        <ul>
                                            <li><a href="{{ path('index') }}">Accueil</a>
                                                <ul class="dropdown_menu">

                                                </ul>
                                            </li>
                                            <li><a href="#">Import & Export</a></li>
                                            <li><a href="{{ path('investissement') }}">Investissements</a>
                                                <ul class="dropdown_menu">
                                                    <li><a href="{{ path('property') }}">Immobiliers</a></li>
                                                    <li><a href="#">Crypto-monnaie</a></li>

                                                </ul>
                                            </li>


                                            <li><a href="#">Web service</a>
                                                <ul class="dropdown_menu">

                                                </ul>
                                            </li>
                                            <li><a href="{{ path('contact') }}"> Contact</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Mobile menu start -->
                <div class="mobile-menu-area hidden-lg hidden-md">
                    <div class="container">
                        <div class="col-md-12">
                            <nav id="dropdown">
                                <ul>
                                    <li><a href="{{ path('index') }}">Accueil</a>
                                        <ul>
                                        </ul>
                                    </li>
                                    <li><a href="#">Nos Services</a></li>
                                    <li><a href="{{ path('investissement') }}"> Investissements</a>
                                        <ul>
                                        </ul>
                                    </li>
                                    <li><a href="{{ path('property') }}">Immobiliers</a>
                                    </li>
                                    <li><a href="{{ path('contact') }}"> Contact</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
                <!-- Mobile menu end -->
            </div>
            <div class="header-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-md-12 col-sm-12 col-xs-12">

                            <div class="add-property">
                                <a href="{{ path('add_property') }}">Ajouter une propriété</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {% for type, messages in app.session.flashbag.all %}
                    {% for message in messages %}
                        <div class="alert alert-{{ type ? type : '' }}">
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
        </div>



        {% block content %}{% endblock %}

        <!--Brand section end-->

            <!--Footer bottom start-->
            <div class="footer-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7 col-sm-12 col-xs-12">
                            <div class="footer-menu">
                                <ul>
                                    <li><a href="{{ path('index')}}">Accueil</a></li>
                                    <li><a href="#">Nos Services</a></li>
                                    <li><a href="{{ path('investissement') }}">Investissements</a></li>
                                    <li><a href="{{ path('property') }}">Immobiliers</a></li>
                                    <li><a href="{{ path('contact') }}">Contact </a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-5 col-sm-12 col-xs-12">
                            <div class="copyright">
                                <p>Copyright <i class="fa fa-copyright"></i> 2018 <a href="#">Iboh Company</a>. All rights
                                    reserved.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--Footer bottom end-->
        </footer>
    </div>


    <!-- Placed js at the end of the document so the pages load faster -->

    <!-- Map js code here -->
    {#<script type="text/javascript"#}
            {#src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAj9b_nyz33KEaocu6ZOXRgqwwUZkDVEAw"></script>
    <script src="{{ asset('js/map.js') }}"></script>#}

    <!-- All jquery file included here -->
    <script src="{{ asset('js/vendor/jquery-1.12.0.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/jquery.nivo.slider.pack.js') }}"></script>
    <script src="{{ asset('js/waypoints.min.js') }}"></script>
    <script src="{{ asset('js/jquery.counterup.min.js') }}"></script>
    <script src="{{ asset('js/ajax-mail.js') }}"></script>
    <script src="{{ asset('js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('js/jquery.magnific-popup.js') }}"></script>
    <script src="{{ asset('js/plugins.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>

    {% block js %}{% endblock %}
</body>
</html>