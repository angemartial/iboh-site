<!doctype html>
<html class="no-js" lang="zxx">

<head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <title>{% block title %} Iboh Immobilier {% endblock %}</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Place favicon.ico in the root directory -->
    <link href="#" type="img/x-icon" rel="shortcut icon">
    <!-- All css files are included here. -->
    <link rel="stylesheet" href="{{ asset('css/bootstrap.min.css') }}">
    <link rel="stylesheet" href="{{ asset('css/core.css') }}">
    <link rel="stylesheet" href="{{ asset('css/shortcode/shortcodes.css') }}">
    <link rel="stylesheet" href="{{ asset('style.css') }}">
    <link rel="stylesheet" href="{{ asset('css/responsive.css') }}">
    <!-- customizer style css -->
    <link rel="stylesheet" href="{{ asset('css/style-customizer.css') }}">
    <link href="#" data-style="styles" rel="stylesheet">

    {% block css %}{% endblock %}

    <!-- Modernizr JS -->
    <script src="{{ asset('js/vendor/modernizr-2.8.3.min.js') }}"></script>
</head>

<body>
<!--[if lt IE 8]>
    <p class="browserupgrade">You are using an <strong>outdated</strong> browser. Please <a href="http://browsehappy.com/">upgrade
        your browser</a> to improve your experience.</p>
    <![endif]-->

    <div class="wrapper white_bg">
        <!--Header section start-->
        <header class="header">
            <div class="header-top-1">
                <div class="container">
                    <div class="row">
                        <div class="col-md-4 col-sm-4 hidden-xs">

                        </div>
                        <div class="col-md-8 col-sm-8 col-xs-12">
                            <div class="header-1-top-inner">
                                <div class="header-topbar-menu">
                                    <ul>
                                        <li><a href="#"></a></li>

                                        {% if is_granted('ROLE_USER') %}
                                            <li><a href="{{ path('myaccount', {username: app.user.username }) }}">Connecté en tant que {{ app.user.username }} !</a></li>
                                            <li><a href="{{ path('fos_user_security_logout') }}">Déconnexion</a></li>
                                        {% else %}
                                            <li><a href="{{ path('dup_user_security_login') }}">Se connecter</a></li>
                                            <li><a href="{{ path('register') }}">S'inscrire</a></li>
                                        {% endif %}

                                    </ul>
                                </div>
                                <div class="header-search">
                                    <div class="search-icon">
                                        <a href="#"><img src="#" alt=""></a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="header-top sticky-header">
                <div class="container">
                    <div class="row">
                        <div class="col-md-2 col-sm-4 col-xs-6">
                            <div class="logo">
                                <a href="{{ path('index') }}"><img src="{{ asset('img/logo/logo1.fw.png') }}" alt=""></a>
                            </div>
                        </div>
                        <div class="col-md-10 hidden-sm hidden-xs">
                            <div class="mgea-full-width">
                                <div class="header-menu">
                                    <nav>
                                        <ul>
                                            <li><a href="{{ path('index') }}">Accueil</a>
                                                <ul class="dropdown_menu">

                                                </ul>
                                            </li>
                                            <li><a href="#">Achetez</a></li>
                                            <li><a href="#">Louez</a>
                                                {#<ul class="dropdown_menu">#}
                                                    {#<li><a href="{{ path('property') }}">Immobiliers</a></li>#}
                                                    {#<li><a href="#">Crypto-monnaie</a></li>#}
                                                {#</ul>#}
                                            </li>


                                            <li><a href="#">Vendez</a>
                                                <ul class="dropdown_menu">

                                                </ul>
                                            </li>
                                            <li><a href="{{ path('contact') }}"> Contactez-nous</a></li>
                                        </ul>
                                    </nav>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <!-- Mobile menu start -->
                <div class="mobile-menu-area hidden-lg hidden-md">
                    <div class="container">
                        <div class="col-md-12">
                            <nav id="dropdown">
                                <ul>
                                    <li><a href="{{ path('index') }}">Accueil</a>
                                        <ul>
                                        </ul>
                                    </li>
                                    <li><a href="#">Achetez</a></li>
                                    <li><a href="#"> Louez</a>
                                        <ul>
                                        </ul>
                                    </li>
                                    <li><a href="#">Vendez</a>
                                    </li>
                                    <li><a href="{{ path('contact') }}"> Contactez-nous</a></li>
                                </ul>
                            </nav>
                        </div>
                    </div>
                </div>
                <!-- Mobile menu end -->
            </div>

            {% if is_granted('ROLE_ADMIN') %}
                <div class="header-bottom">
                    <div class="container">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">

                                <div class="add-property">
                                    <a href="{{ path('add_property') }}">Ajouter une propriété</a>
                                </div>
                                <div class="add-property">
                                    <a href="{{ path('add_project') }}">Ajouter un projet</a>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            {% endif %}


        </header>

        <div class="row">
            <div class="col-md-6 col-md-offset-3">
                {% for type, messages in app.session.flashbag.all %}
                    {% for message in messages %}
                        <div class="alert alert-{{ type ? type : '' }}">
                            {{ message }}
                        </div>
                    {% endfor %}
                {% endfor %}
            </div>
        </div>



        {% block content %}{% endblock %}

        <!--Brand section end-->

            <!--Footer bottom start-->
            <div class="footer-bottom">
                <div class="container">
                    <div class="row">
                        <div class="col-md-7 col-sm-12 col-xs-12">
                            <div class="footer-menu">
                                <ul>
                                    <li><a href="{{ path('index')}}">Accueil</a></li>
                                    <li><a href="#">Achetez</a></li>
                                    <li><a href="#">Louez</a></li>
                                    <li><a href="#">Vendez</a></li>
                                    <li><a href="{{ path('contact') }}">Contactez-nous </a></li>
                                </ul>
                            </div>
                        </div>
                        <div class="col-md-5 col-sm-12 col-xs-12">
                            <div class="copyright">
                                <p>Copyright <i class="fa fa-copyright"></i> 2018 <a href="#">Iboh Company</a></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <!--Footer bottom end-->
        </footer>
    </div>
    <!-- Placed js at the end of the document so the pages load faster -->

    <!-- Map js code here -->
    {#<script type="text/javascript"#}
            {#src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAj9b_nyz33KEaocu6ZOXRgqwwUZkDVEAw"></script>
    <script src="{{ asset('js/map.js') }}"></script>#}

    <!-- All jquery file included here -->
    <script src="{{ asset('js/vendor/jquery-1.12.0.min.js') }}"></script>
    <script src="{{ asset('js/bootstrap.min.js') }}"></script>
    <script src="{{ asset('js/jquery.nivo.slider.pack.js') }}"></script>
    <script src="{{ asset('js/waypoints.min.js') }}"></script>
    <script src="{{ asset('js/jquery.counterup.min.js') }}"></script>
    <script src="{{ asset('js/ajax-mail.js') }}"></script>
    <script src="{{ asset('js/owl.carousel.min.js') }}"></script>
    <script src="{{ asset('js/jquery.magnific-popup.js') }}"></script>
    <script src="{{ asset('js/plugins.js') }}"></script>
    <script src="{{ asset('js/main.js') }}"></script>

    {% block js %}{% endblock %}
</body>
</html>