{% extends 'layout.html.twig' %}

{% block content %}
    <!--slider section start-->
    <div class="slider-container overlay">
        <!-- Slider Image -->
        <div id="mainSlider" class="nivoSlider slider-image">
            <img src="{{ asset('img/slider/22.jpg') }}" alt="" title="#htmlcaption1"/>
        </div>
        <!-- Slider Caption 1 -->
        <div id="htmlcaption1" class="nivo-html-caption slider-caption-1">
            <div class="display-table">
                <div class="display-tablecell">
                    <div class="container ">
                        <div class="row">
                            <div class="col-md-12 col-sm-12 col-xs-12">
                                <div class="slide1-text">
                                    <div class="middle-text">
                                        <div class="title-1 wow fadeUp" data-wow-duration="1.2s" data-wow-delay="0.1s">
                                            <h3>Voulez-vous acheter, louer ou vendre une maison?</h3>
                                        </div>
                                        <div class="title-2 wow fadeUp col-md-8" data-wow-duration="1.3s" data-wow-delay="0.2s">
                                            <h1><span>IBOH COMPANY</span><br>
                                                vous accompagne jusqu'à la réalisation de votre projet </h1>
                                            <div class="contact-us wow fadeUp" data-wow-duration="1.5s"
                                                 data-wow-delay=".5s">
                                                <a href="{{ path('contact') }}">CONTACTEZ-NOUS</a>
                                            </div>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
        <!--Find home area start-->

        <div class="finde-home-postion">
            <div class="container">
                <div class="find-home-box postion">
                    <div class="find-home-box-inner">
                        <div class="find-home-title">
                            <!-- INSERTION ZONE DE RECHERCHE-->
                            <aside class="single-side-box search-property">
                                <div class="aside-title">
                                    <h3>RECHERCHER UNE MAISON</h3>
                                </div>
                                {{ render(controller('AppBundle\\Controller\\DefaultController::propertySearchAction')) }}
                            </aside><!-- FIN INSERTION ZONE RECHERCHE-->
                        </div>
                    </div>
                </div>
            </div>
        </div>

        <!--Find home area end-->

    <!--slider section end-->

    <!--Services section start-->
    <div class="services-section ptb-50 bg-1">
        <div class="container">
            <div class="row">
                <div class="col-md-12 ">
                    <div class="section-title text-center">
                        <h3>Nos Services</h3>
                        {#<p>IBOH Company vous propose un accompagnement informatique à travers ses solutions web#}
