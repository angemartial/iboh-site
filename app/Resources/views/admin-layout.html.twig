                            <li><a href="#">Liste des utilisateurs</a></li>
                            <li><a href="#">Ajouter un utilisateur</a></li>
                            <li><a href="#">Créer une catégorie</a></li>

                        </ul>
                    </li>

                    <li class="top">
                        <a href="{{ path('dup_user_logout') }}" class="has-arrow"><i class="icon-docs"></i><span>Déconnexion</span></a>
                    </li>

                </ul>
            </nav>
        </div>
